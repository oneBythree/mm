@mixin funcHeight($height){
    height:$height;
    line-height:$height;
}
@mixin border-top($color){
    border-top: 1px solid $color;
}
@mixin border-bottom($color){
    border-bottom: 1px solid $color;
    @extend .border-box;
}
@mixin border-bottom2top($color){
    border-bottom: 1px solid $color;
    border-top: 1px solid $color;
    @extend .border-box;
}
@mixin border-radius2all($color,$size){
    border: 1px solid $color;
    border-radius: $size;
    @extend .border-box;
} 
