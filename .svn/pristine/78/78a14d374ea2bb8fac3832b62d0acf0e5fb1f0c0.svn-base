<div >
  <div class="m-crumbs">您当前的位置：<a class="m-link" href="javascript:;">商户备案</a></div>
  <form class="form-inline m-line-form ">
    <div class="form-group">
      <label for="inputId" ng-init="tagDropList=['大润发','物美','中百','华联']">商户名称</label>
      <div class="inblock small" drop-search create-swtich=false drop-list="tagDropList" for-value=""></div> 
    </div>
    <div class="form-group">
      <label for="inputId">商户编码</label>
      <input type="text" class="form-control m-input w100" id="inputId" ng-model="goodsId">
    </div>
    <div class="form-group">
      <label >备案时间</label>
      <div class="m-icon-date">
        <span for="date1"><i class="iconfont icon-riqi"></i></span>
        <input id="date1"  bs-Datepicker type="text" class="w100 date-defalut" placeholder="请选择时间"  readonly>
      </div>
    </div>
    <div class="form-group">
      <label for="inputId" ng-init="cDropList=['北京','上海','深圳','天津','武汉']">归属区县</label>
      <div class="inblock small" drop-search create-swtich=false drop-list="cDropList" for-value=""></div>  
    </div>

    <button type="submit" class="btn m-btn-icon" ng-click="search()"><i class="iconfont icon-search"></i> </button>
    <button type="submit" class="btn m-btn-icon" ng-click="reset()"><i class="iconfont icon-cancel"></i> </button>
    <div style="height:10px;"></div>
    <div class="form-group">
      <label >企业查询</label>
      <select class="form-control">
        <option value="-1"></option>
        <option value="1">民企</option>
        <option value="2">国企</option>
        <option value="3">合资</option>
      </select>
    </div>
    <div class="form-group">
      <label for="inputId">节点名称</label>
      <input type="text" class="form-control m-input w100" id="inputId" ng-model="goodsId">
    </div>
  </form>

  <div class="btn-group">
    <a href="#/record/add" class="btn m-btn">添加</a>
    <a href="#/record/giveic" class="btn m-btn">发放IC卡</a>
    <a href="#/record/icinfo" class="btn m-btn">IC卡信息</a>
    <a href="#/goods/add" class="btn m-btn">编辑</a>
    <a href="#/goods/add" class="btn m-btn">删除</a>
  </div>

  <table class="table table-bordered m-table">
    <thead>
      <tr>
        <th><input type="checkbox" ng-model="selectAll" name="sort">序号</th>
        <th>商户编码</th>
        <th>商户名称</th>
        <th>节点名称</th>
        <th>商户类型</th>
        <th>法人代表</th>
        <th>联系电话</th>
        <th>客户性质</th>
        <th>工商注册登记号</th>
        <th>备案日期</th>
        <th>所属区县</th>
        <th>经营地址</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td><input type="checkbox" ng-checked="selectAll" name="sort">11</td>
        <td>2</td>
        <td>3</td>
        <td>4</td>
        <td>5</td>
        <td>6</td>
        <td>7</td>
        <td>8</td>
        <td>9</td>
        <th>1</th>
        <th>2</th>
        <th>3</th>  
      </tr>
      <tr>
        <td><input type="checkbox" ng-checked="selectAll" name="sort">22</td>
        <td>2</td>
        <td>3</td>
        <td>4</td>
        <td>5</td>
        <td>6</td>
        <td>7</td>
        <td>8</td>
        <td>9</td>
        <th>1</th>
        <th>2</th>
        <th>3</th>  
      </tr>
      <tr>
        <td><input type="checkbox" ng-checked="selectAll" name="sort">33</td>
        <td>2</td>
        <td>3</td>
        <td>4</td>
        <td>5</td>
        <td>6</td>
        <td>7</td>
        <td>8</td>
        <td>9</td>
        <th>1</th>
        <th>2</th>
        <th>3</th>  
      </tr>
    </tbody>
  </table>
</div>
