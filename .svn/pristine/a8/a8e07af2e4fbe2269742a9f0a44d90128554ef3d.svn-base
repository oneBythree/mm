div.container-index {
    display: table;
    width: 100%;
    height: 100%;
    vertical-align: bottom;
}


/*侧边栏*/

div.logo-info {
    height: 260px;
    text-align: center;
}

div.logo-info h2 {
    padding: 86px 0 46px;
    color: #1fb5ad;
    font-size: 30px;
}

div.logo-info img {
    width: 86px;
    height: auto;
    margin: 55px 0 25px 0;
}

div.logo-info p {
    color: #fff;
    font-size: 14px;
}

.sideBar {
    position: relative;
    display: table-cell;
    vertical-align: top;
    z-index: 1000;
}

div.container-main {
    display: table-cell;
    vertical-align: top;
}

.odal-backdrop {
    z-index: 100!important;
}


/*hover内容*/

.userinfo-bar li.msg:hover ul.hover-ul {
    display: block;
    /*    -webkit-transition: height 1s ease-in-out;
    -moz-transition: height 1s ease-in-out;
    -o-transition: height 1s ease-in-out;
    transition: height 1s ease-in-out;*/
}

ul.hover-ul {
    display: none;
    position: absolute;
    left: -1px;
    top: 60px;
    padding: 0 5px;
    text-align: left;
    background: #fff;
    width: 275px;
    border-left: 1px solid #ccc;
    border-right: 1px solid #ccc;
}

ul.hover-ul>li.title {
    font-size: 14px;
    color: #444;
    line-height: 24px!important;
}

ul.hover-ul>li.list {
    line-height: 24px!important;
    background: #f1f1f1;
    font-size: 12px;
    padding: 2px 8px;
    border-radius: 2px;
    margin-bottom: 10px;
}


/*抬头导航*/

div.userinfo-bar {
    position: fixed;
    top: 0;
    left: 0;
    padding-left: 310px;
    width: 100%;
    height: 60px;
    background: #fff;
    text-align: right;
    border-bottom: 1px solid #cdcdcd;
    z-index: 999;
}

div.userinfo-bar ol {
    display: inline-block;
    height: 100%;
    margin-bottom: 0;
}

div.userinfo-bar>ol>li {
    float: left;
    position: relative;
    line-height: 60px;
    border-left: 1px solid #ccc;
    height: 100%;
    text-align: center;
    color: #666;
}


/*div.userinfo-bar li.power {
    width: 60px;
}

div.userinfo-bar li.power {
    width: 60px;
}

div.userinfo-bar li.power a {
    display: inline-block;
}

div.userinfo-bar i.icon-power {
    color: #666;
    font-size: 20px;
}*/

div.userinfo-bar li.info {
    width: 180px;
    padding: 0 20px;
    font-size: 20px;
}

div.userinfo-bar li.info>ul.info-manager {
    position: absolute;
    width: 238px;
    left: -1px;
}

div.userinfo-bar li.info>span.fr {
    font-size: 14px;
    font-family: "Microsoft YaHei";
    cursor: pointer;
}

div.userinfo-bar li.msg {
    width: 70px;
}

div.userinfo-bar li.msg a {
    position: relative;
    display: inline-block;
}

a.msg-warp i.iconfont {
    font-size: 30px!important;
}

a.remind-warp sup {
    background: #fcb322;
}

a.msg-warp sup {
    background: #f86b4f;
}

div.userinfo-bar li.msg i.iconfont {
    display: inline-block;
    color: #666;
    font-size: 26px;
}

div.userinfo-bar li.msg sup {
    position: absolute;
    display: inline-block;
    width: 26px;
    height: 20px;
    line-height: 20px;
    font-size: 12px;
    color: #fff;
    right: -13px;
    top: 10px;
    border-radius: 8px;
}

.m-crumbs {
    color: #323232;
    text-indent: 2px;
    padding: 20px 0;
    margin-top: 10px;
    font-size: 16px;
    border-bottom: 1px solid #cccccc;
}

.m-link,
.m-link:hover {
    color: #1fb5ad!important;
}

.m-line-form {
    padding: 30px 0;
}

.m-line-form label {
    font-weight: normal!important;
    margin-right: 12px;
}

.m-line-form .form-group {
    margin-right: 20px;
}

.m-icon-date {
    display: inline-block;
    height: 36px;
    line-height: 36px;
    border: 1px solid #ccc;
    border-radius: 4px;
    background: #fff;
    overflow: hidden;
    vertical-align: middle;
}

.m-icon-date i.iconfont {
    font-size: 14px;
    color: #999;
}


/*tab标签*/

div.tab-main {
    padding: 10px 38px;
    margin-top: 60px;
}


/*图标字体*/

.m-btn-icon {
    padding: 0 22px;
    background: #1fb5ad;
}

.m-btn-icon i.iconfont {
    font-size: 20px;
    height: 36px;
    line-height: 36px;
    color: #fff;
}

.m-input {
    height: 36px;
    line-height: 36px;
    padding: 0 12px!important;
}

.m-icon-date input[type="text"] {
    border: none;
    height: 100%;
    vertical-align: top;
    font-size: 14px;
}

.w80 {
    width: 80px!important;
}

.w100 {
    width: 100px!important;
}

.w180 {
    width: 180px!important;
}

.w240 {
    width: 240px!important;
}


/*表单table*/

table.m-table {
    background: #fff;
}

table.table.m-table {
    width: 100%;
    table-layout: fixed;
}

table.m-table thead th {
    background: #1fb5ad;
    color: #fff;
    text-align: center;
    vertical-align: middle!important;
}

table.m-table td,
table.m-table th {
    text-align: center!important;
    vertical-align: middle!important;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}


/*分页*/

nav.m-page {
    text-align: right;
}

nav.m-page ul {
    margin: 0 0 20px 0!important;
}

nav.m-page ul a {
    color: #323232;
}

.marg-b20 {
    margin-bottom: 20px;
}


/*响应式1440以下的屏幕*/

@media screen and (max-width: 1440px) {
    div#sideBar {
        width: 248px;
    }
    div.logo-info h2 {
        padding: 68px 0 36px;
        font-size: 24px;
    }
    div.logo-info img {
        width: 68px;
        margin: 44px 0 20px;
    }
    div.logo-info {
        height: 198px
    }
    form.m-h-form.form-horizontal {
        width: 720px;
    }
    div.container-main {
        /*    left:248px;
    padding-right: 248px;*/
    }
    ul.side-menu a.tag-menu {
        padding-left: 27px;
        font-size: 16px;
    }
    ul.side-menu a.tag-menu>i.iconfont {
        font-size: 16px;
        margin-left: 12px;
    }
    ul.side-menu ol a {
        font-size: 14px;
        padding: 0 16px 0 38px;
    }
    ul.side-menu ol a>i.iconfont:not(.icon-right-arrow) {
        font-size: 14px;
        margin-right: 5px;
    }
    ul.side-menu a>i.icon-right-arrow {
        font-size: 12px;
    }
    div.tab-main {
        padding: 10px 30px;
    }
    .m-crumbs {
        padding: 16px 0;
        margin-top: 8px;
        font-size: 14px;
    }
    .m-line-form {
        padding: 24px 0;
    }
    .m-line-form label {
        margin-right: 8px;
    }
    .m-line-form .form-group {
        margin-right: 16px;
    }
    .m-btn-icon {
        padding: 0 17px;
    }
    .m-btn-icon i.iconfont {
        font-size: 16px;
        height: 28px;
        line-height: 28px;
    }
    .m-input {
        height: 28px;
        line-height: 28px;
        font-size: 12px;
    }
    .m-icon-date {
        height: 28px;
        line-height: 28px;
    }
    .m-icon-date span {
        display: inline-block;
        width: 28px;
        text-align: center;
    }
    .m-icon-date input[type="text"] {
        font-size: 12px;
    }
}


/**/

em.test-user {
    display: inline-block;
    width: 40px;
    height: 40px;
    line-height: 40px;
    border-radius: 50%;
    background: url(../images/userinfo.png);
    background-size: 100% 100%;
    margin-top: 10px;
}

dl.sidemenu dt.ranks a {
    display: block;
    padding: 0 34px;
    color: #95a0aa;
    height: 50px;
    line-height: 50px;
    border: 1px solid #3b3b43;
}


/*表格*/

.chart {
    box-shadow: 0 0 5px 0 #888888;
    -moz-box-shadow: 0 0 5px 0 #888888;
    -webkit-box-shadow: 0 0 5px 0 #888888;
}

.chart-canvas {
    padding-top: 16px;
    width: 100%;
    height: 400px;
    background: #fff;
}


/*复选框 单选*/

label.m-inline>input[type="radio"]:checked+em {
    color: #1fb5ad;
}

label.m-inline>input[type="radio"] {
    margin: -4px 5px 0 0px;
}

label.m-inline {
    margin-right: 10px;
    padding: 0;
    cursor: pointer;
}

label.inblock-check {
    padding-right: 10px;
}


/*表单*/

form.m-h-form.form-horizontal {
    width: 860px;
    margin: 40px auto;
}

form.m-h-form.form-horizontal div.col-sm-10 {
    position: relative;
}

form.m-h-form.form-horizontal i.red {
    position: absolute;
    right: -6px;
    top: 50%;
    font-size: 12px;
    margin-top: -6px;
    color: red;
}

#sideBar {
    display: table-cell;
    width: 310px;
    background: #323232;
}


/*验证提示添加样式*/

select.form-control.selectpicker+span.msg-box.n-right {
    position: absolute;
    right: 15px;
}

form.form-horizontal.m-h-form span.msg-box.n-right>span[role="alert"] {
    background: #f1f1f1;
}

form.form-horizontal.m-h-form >div.form-group>label {
    font-weight: normal;
}
