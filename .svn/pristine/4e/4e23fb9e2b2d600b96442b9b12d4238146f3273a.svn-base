    <script type="text/javascript">
    $('#listshow a.item').on('click', function(event) {
        $('#listshow a.item').removeClass('active');
        $(this).addClass('active');
        var curele = $(this).nextAll('ul');
        var thisList1 = $(this).parents('.sub1');
        $('.sub1').not(thisList1).slideUp();
        if (curele !== null) {
            if (curele.is(':hidden')) {
                curele.slideDown();
            } else {
                curele.slideUp();
            };
        };
        sessionStorage.removeItem("silderLevel");
        var level = $(this).attr('data-level');
        console.log(level);
        sessionStorage.setItem('silderLevel',level);
    });
    
    $(document).ready(function () {
        var sLevel = sessionStorage.getItem('silderLevel');
        var level = (sLevel == undefined || sLevel == '')? '0-0-0' : sLevel;
	    console.log(sessionStorage.getItem('silderLevel'));
		var thisEl = $("[data-level="+level+"]");	
		$('.sub1').not($("[data-level=" + level + "]").parents('.sub1')).hide();
		$("[data-level=" + level + "]").parents('.sub1,.sub2').show()
		$('#list a').removeClass('active');
		thisEl.addClass('active');
		if(thisEl.next().hasClass('sub2')||thisEl.next().hasClass('sub1')){
			thisEl.next().show();
		}
	})
    </script>

